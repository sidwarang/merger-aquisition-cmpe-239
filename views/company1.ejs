    <script>
    
    
    	var company = angular.module("company1_app",["ng-fusioncharts"]);
        company.controller('company1_controller', function($scope,$http,$window){
        
        	
        	$http({
    			method:'POST',
    			url:'/getCompanyDetails',
    			data:{"id":"1"}
    		}).then(function(res){
    			if(res.data.status == "200"){
					//alert(comp1[0].Company_Name.toString().toUpperCase());
    				data = res.data.companyDetails.domain;
					$scope.location = res.data.companyDetails.location;
    				$scope.company1 = res.data.company1Name.toString().toUpperCase();
    				$scope.company2 = res.data.company2Name.toString().toUpperCase();
    				$scope.chart = {"caption":"Revenue Increment in different domains","subCaption":res.data.companyDetails.companyName.toString().toUpperCase(),"xAxisName":"Domain Name","yAxisName":"Revenue percent increase","data":data};
    				$scope.chart2_Create();
    				chart1_Create(res.data.comp1);    				
    				chart3_Create(res.data.comp1,res.data.tweets,res.data.polarity);
					$scope.createChart4();
    			}else{
    				
    			}
    			
    		})
        	
    		 $scope.chart2_Create = function(){
       		 FusionCharts.ready(function(){
       			  var chart2 = new FusionCharts({
       			      "type": "column2d",
       			      "renderAt": "container2",
       			      "width": "452",
       			      "height": "400",
       			      "dataFormat": "json",
       			      "dataSource":  {
       			    	   "chart": {
       			    	        "caption":  $scope.chart.caption, 
       			    	        "subCaption": $scope.chart.subCaption,
       			    	        "xAxisName": $scope.chart.xAxisName,
       			    	        "yAxisName": $scope.chart.yAxisName,
       			    	        "lineThickness": "2",
       			    	        "paletteColors": "#0075c2",
       			    	        "baseFontColor": "#333333",
       			    	        "baseFont": "Helvetica Neue,Arial",
       			    	        "captionFontSize": "14",
       			    	        "subcaptionFontSize": "14",
       			    	        "subcaptionFontBold": "0",
       			    	        "showBorder": "0",
       			    	        "bgColor": "#ffffff",
       			    	        "showShadow": "0",
       			    	        "canvasBgColor": "#ffffff",
       			    	        "canvasBorderAlpha": "0",
       			    	        "divlineAlpha": "100",
       			    	        "divlineColor": "#999999",
       			    	        "divlineThickness": "1",
       			    	        "divLineDashed": "1",
       			    	        "divLineDashLen": "1",
       			    	        "divLineGapLen": "1",
       			    	        "showXAxisLine": "1",
       			    	        "xAxisLineThickness": "1",
       			    	        "xAxisLineColor": "#999999",
       			    	        "showAlternateHGridColor": "0"
       			    	    },
       			    	    "data":$scope.chart.data
       			    	   }
       			});
       			chart2.render();
       			}) 
       	} 
		
		
		$scope.createChart4 = function(){
		
		FusionCharts.ready(function () {
		var locationMap = new FusionCharts({
        type: 'maps/usa',
        renderAt: 'container4',
        width: '452',
        height: '400',
        dataFormat: 'json',
        dataSource: {
            "chart": {
                "caption": "Company Location",
                "subcaption": "", 
                "borderColor": "#DDDDDDD",
                "showLabels": "1",               
                
                "numberScaleValue": "1,1000,1000",
                "numberScaleUnit": "K,M,B",
                "numberPrefix": "$",
                "entityFillHoverColor": "#d35400",
                "entityFillHoverAlpha": "30",
                "theme" : "fint"
            },
            "colorrange": {
                "gradient": "1",
                "startLabel": "",
                "endLabel": "",
                "code": "#f39c12",
                "minvalue": "",
                "maxValue": "" 
            },
            "data": $scope.location
        }
    });
    locationMap.render();
});
		
		
		}
		

        });
    	
    
        
        function chart1_Create(comp1){
        	
        	
        	var chart1 = new Highcharts.Chart({
                chart: {
                    type: 'column',
                    renderTo: 'container1'
                },
				title: {
                    text: 'Stock data'
                },
                subtitle: {
                    text:  comp1[0].Company_Name.toString().toUpperCase()
                },
                xAxis: {
                    categories: ['Current Assests',
                        'Liability',
                        'EBITDA',
                        'Gross Margin',
                        'Cash Equivalent']
                },
                yAxis: {
                    min: 0,
                    title: {
                        text: 'Stock Data'
                    },
                    stackLabels: {
                        enabled: true,
                        style: {
                            fontWeight: 'bold',
                            color: (Highcharts.theme && Highcharts.theme.textColor) || 'gray'
                        }
                    }
                },
                legend: {
                    align: 'right',
                    x: -30,
                    verticalAlign: 'top',
                    y: 25,
                    floating: true,
                    backgroundColor: (Highcharts.theme && Highcharts.theme.background2) || 'white',
                    borderColor: '#CCC',
                    borderWidth: 1,
                    shadow: false
                },
                tooltip: {
                    headerFormat: '<b>{point.x}</b><br/>',
                    pointFormat: '{series.name}: {point.y}<br/>Total: {point.stackTotal}'
                },
                plotOptions: {
                    column: {
                        stacking: 'normal',
                        dataLabels: {
                            enabled: true,
                            color: (Highcharts.theme && Highcharts.theme.dataLabelsColor) || 'white',
                            style: {
                                textShadow: '0 0 3px black'
                            }
                        }
                    }
                },
                series: [{
                    name:  comp1[0].Company_Name.toString().toUpperCase() ,
                    data: [ comp1[0].Total_Current_Assets, comp1[0].Total_Liabilities , comp1[0].EBITDA , comp1[0].Gross_Margin ,comp1[0].Cash_and_Cash_Equivalents]
                }]
            });
            chart1.render();
       }
    
    
        function chart3_Create(comp1,tweets,polarity){
        	
        	var chart3 = new Highcharts.Chart({
                chart: {
                    renderTo:'container3',
                    backgroundColor: '#EDEEFA',
                    type: 'pie',
                    options3d: {
                        enabled: true,
                        alpha: 45
                    }
                },
                title: {
                    text: 'Tweets Polarity'
                },
                subtitle: {
                    text:  comp1[0].Company_Name.toString().toUpperCase()
                },
                plotOptions: {
                    pie: {
                        innerSize: 100,
                        depth: 45
                    }
                },
                series: [{
                    name: 'Tweets Polarity',
                    data: [
                        ['Neutral', polarity[2]],
                        ['Negative', polarity[1]] ,
                        ['Positive', polarity[0]]


                    ]
                }]
            });
            chart3.render();
         }
    
     </script>
    
    

